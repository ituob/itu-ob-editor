{
  "extends": "./node_modules/electron-webpack/tsconfig-base.json",
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "models/*": ["models/*"]
    },
    "lib": ["es2019", "DOM"],
    "plugins": [{ "name": "typescript-plugin-css-modules" }],
    "jsx": "react",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  }
}
